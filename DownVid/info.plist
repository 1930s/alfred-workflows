				<string>source ./workflowHandler.sh
# addResult "uid" "arg" "title" "subtitle" "icon" "valid" "autocomplete"

# Check if WatchList is being used, and shows options accordingly
watchlist="${HOME}/Library/Application Support/Alfred 2/Workflow Data/com.vitorgalvao.alfred.watchlist/watchlist.txt"

if [[ -f ${watchlist} ]]; then
	addResult "Regular Download" "" "DownVid" "Downloads the video to your Desktop" "icon.png" "yes" ""
	addResult "Download To WatchList" "addToWatchList" "DownVid + WatchList" "Downloads the video to ~/Downloads and adds it to your watchlist" "icon.png" "yes" ""
else
	addResult "Regular Download" "" "DownVid" "Downloads the video to your Desktop" "icon.png" "yes" ""
fi

# get feedback xml
getXMLResults</string>
