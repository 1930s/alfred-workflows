				<string>if [[ "{query}" == "addToWatchList" ]]; then
	watchlist="${HOME}/Library/Application Support/Alfred 2/Workflow Data/com.vitorgalvao.alfred.watchlist/watchlist.txt"

	echo $(python youtube-dl -o "${HOME}/Downloads/%(title)s.%(ext)s" --get-filename "$(pbpaste)") &gt;&gt; "${watchlist}"
	python youtube-dl -o "${HOME}/Downloads/%(title)s.%(ext)s" "$(pbpaste)"

	echo "Done."
else
	python youtube-dl -o "${HOME}/Desktop/%(title)s.%(ext)s" "$(pbpaste)"
	echo "Done."
fi</string>
